[ignore]
# fbjs lib problems
.*/node_modules/fbjs/.*
# radium lib problems
.*/node_modules/radium/.*
.*/node_modules/.*/\(test\).*\.json$
.*/node_modules/promise/.*

[include]

[libs]
flow/.*
flow-typed/.*

[options]
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe
suppress_comment=\\(.\\|\n\\)*\\$FlowIgnore
module.ignore_non_literal_requires=true

[version]
^0.50.0
