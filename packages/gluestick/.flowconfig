[ignore]
# Ignore templates with `@flow` in the header
.*/templates/new/.*\.js
# fbjs lib problems
.*/node_modules/fbjs/.*
# radium lib problems
.*/node_modules/radium/.*

[include]

[libs]
flow/
flow-typed/

[options]
suppress_type=$FlowFixMe
module.ignore_non_literal_requires=true

[version]
^0.38.0
